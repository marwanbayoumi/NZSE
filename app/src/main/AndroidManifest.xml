<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myapplication">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Kunde_Recycler"
            android:parentActivityName=".MainActivity"></activity>
        <activity
            android:name=".Recycler"
            android:label="living space markler"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".NewWohnugsObjektView" />
        <activity
            android:name=".MarklerCardView"
            android:label="@string/title_activity_markler_card_view"
            android:parentActivityName=".Recycler"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
         android:name=".KundeCardView"
         android:label="@string/title_activity_kunde_card_view"
         android:parentActivityName=".Kunde"
         android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Agent"
            android:label="Markler View"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Kunde"
            android:label="Kunde View"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".CardViewInterface" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>